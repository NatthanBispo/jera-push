<table>
  <thead>
    <tr>
      <th data-field="id"><%= t('jera_push.admin.messages.attributes.id') %></th>
      <th data-field="name"><%= t('jera_push.admin.messages.attributes.content') %></th>
      <th data-field="price"><%= t('jera_push.admin.messages.attributes.date') %></th>

    </tr>
  </thead>
  <tbody>
    <% @messages.each do |message| %>
      <tr>
        <td><%= message.id %></td>
        <td><%= raw(message.display_content) %></td>
        <td><%= message.display_created_at %></td>
        <td><%= message.show_link %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @messages %>
